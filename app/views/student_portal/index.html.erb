<h1>StudentPortal#index</h1>
<p>Find me in app/views/student_portal/index.html.erb</p>
<%= render :partial search %>
<h2>all found items </h2>
<% @items.each do |item| %>
<h4> <%= link_to "#{item.name}", item %></h4>
<ul>
  <li> <%= item.description %> </li>


  <li> color: <div style="height: 20px; width: 20px; border: 1px solid black; background-color: <%= item.color %>; display: inline-block"> </div> </li>
</ul>
<% end %>
